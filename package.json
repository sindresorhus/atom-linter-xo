{
	"name": "linter-xo",
	"version": "0.30.0",
	"description": "Linter for XO",
	"license": "MIT",
	"repository": "xojs/atom-linter-xo",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "sindresorhus.com"
	},
	"private": true,
	"engines": {
		"atom": ">=1.34.0"
	},
	"scripts": {
		"lint": "xo --ignore='mocks/**/*'",
		"pretest": "npm run lint",
		"test": "nyc ava"
	},
	"keywords": [
		"javascript",
		"linter",
		"eslint",
		"code-style",
		"xo"
	],
	"dependencies": {
		"atom-linter": "^10.0.0",
		"atom-package-deps": "^7.0.0",
		"eslint-rule-documentation": "^1.0.0",
		"load-json-file": "^5.1.0",
		"p-limit": "^2.3.0",
		"pkg-up": "^3.1.0",
		"resolve-from": "^4.0.0",
		"unique-string": "^2.0.0",
		"xo": "^0.39.0"
	},
	"devDependencies": {
		"ava": "^3.13.0",
		"nyc": "^15.1.0",
		"proxyquire": "^2.0.1",
		"text-buffer": "^13.15.2",
		"tmp": "0.0.33"
	},
	"package-deps": [
		{
			"name": "linter",
			"minimumVersion": "2.0.0"
		}
	],
	"providedServices": {
		"linter": {
			"versions": {
				"2.0.0": "provideLinter"
			}
		}
	},
	"nyc": {
		"reporter": [
			"text",
			"lcov"
		]
	},
	"xo": {
		"globals": [
			"atom",
			"window"
		],
		"overrides": [
			{
				"files": "spec/**/*",
				"envs": [
					"atomtest",
					"jasmine"
				]
			}
		]
	}
}
